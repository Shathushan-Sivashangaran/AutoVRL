<robot name="human">
    <link name="human">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename='meshes/human_dummy.stl' scale='0.5 0.5 0.5'/>
            </geometry>
            <material name="orange">
                <color rgba="0.9 0.6 0.1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename='meshes/human_dummy.stl' scale='0.5 0.5 0.5'/>
            </geometry>
        </collision>
        <inertial>
            <mass value="10"/>
            <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2" />
        </inertial>
    </link>
</robot>